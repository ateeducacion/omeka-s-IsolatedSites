{
    "name": "ateeducacion/omeka-s-isolatedsites",
    "description": "Filtes out from the admin browser sites where the logged in user has no role",
    "type": "omeka-s-module",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Área de Tecnología Educativa",
            "email": "ate.educacion@gobiernodecanarias.org"
        }
    ],
    "homepage": "https://github.com/ateeducacion/omeka-s-IsolatedSites",
    "support": {
        "issues": "https://github.com/ateeducacion/omeka-s-IsolatedSites/issues"
    },
    "keywords": [
        "Omeka S",
        "module",
        "IsolatedSites"
    ],
    "require": {
        "php": ">=7.4"
    },
    "config": {
        "platform": {
            "php": "7.4"
        },
        "allow-plugins": {
            "omeka/composer-addon-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "squizlabs/php_codesniffer": "*",
        "phpunit/phpunit": "*",
        "zerocrates/extract-tagged-strings": "dev-master",
        "laminas/laminas-form": "^3.4",
        "laminas/laminas-permissions-acl": "*",
        "laminas/laminas-eventmanager": "*",
        "laminas/laminas-authentication": "*",
        "doctrine/orm": "^2.10"

    },
    "autoload": {
        "psr-4": {
            "IsolatedSites\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IsolatedSitesTest\\": "test/"
        }
    },
    "repositories": [
        {
        "type": "vcs",
        "url": "https://github.com/zerocrates/extract-tagged-strings.git"
        }
    ]
}
